{
  "cli": {
    "version": ">=15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "_shared": {
      "autoIncrement": true,
      "node": "20.18.0",
      "resourceClass": "large",
      "credentialsSource": "remote"
    },
    "development": {
      "extends": "_shared",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "environment": "development",
      "ios": {
        "resourceClass": "m-medium",
        "credentialsSource": "remote",
        "entitlements": {
          "com.apple.security.application-groups": [
            "group.com.convos.preview",
            "group.com.convos.dev",
            "group.com.convos.preview.nse"
          ],
          "keychain-access-groups": [
            "$(AppIdentifierPrefix)group.com.convos.preview",
            "$(AppIdentifierPrefix)com.convos.preview.ConvosNSE",
            "$(AppIdentifierPrefix)com.convos.preview"
          ]
        }
      }
    },
    "preview": {
      "extends": "_shared",
      "channel": "preview",
      "environment": "preview",
      "ios": {
        "resourceClass": "m-medium",
        "credentialsSource": "remote",
        "entitlements": {
          "com.apple.security.application-groups": [
            "group.com.convos.preview",
            "group.com.convos.dev",
            "group.com.convos.preview.nse"
          ],
          "keychain-access-groups": [
            "$(AppIdentifierPrefix)group.com.convos.preview",
            "$(AppIdentifierPrefix)com.convos.preview.ConvosNSE",
            "$(AppIdentifierPrefix)com.convos.preview"
          ]
        }
      }
    },
    "production": {
      "extends": "_shared",
      "channel": "production",
      "environment": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "resourceClass": "m-medium",
        "credentialsSource": "remote",
        "entitlements": {
          "com.apple.security.application-groups": [
            "group.com.convos.preview",
            "group.com.convos.dev",
            "group.com.convos.preview.nse"
          ],
          "keychain-access-groups": [
            "$(AppIdentifierPrefix)group.com.convos.preview",
            "$(AppIdentifierPrefix)com.convos.preview.ConvosNSE",
            "$(AppIdentifierPrefix)com.convos.preview"
          ]
        }
      }
    }
  },
  "submit": {
    "preview": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6743806056",
        "appleTeamId": "FY4NZR34Z3"
      }
    },
    "production": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6744714645",
        "appleTeamId": "FY4NZR34Z3"
      }
    }
  }
}
